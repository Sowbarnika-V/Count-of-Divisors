import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		int l,m;
		Scanner sc = new Scanner(System.in);
		l=sc.nextInt();
		m=sc.nextInt();
		System.out.println(gcd(l,m));
	}
	private static int gcd(int num1,int num2)
	{
	if(num2==0)
		return num1;
	return gcd(num2,num1%num2);
	}
}

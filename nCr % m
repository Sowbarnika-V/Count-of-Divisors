public class Main
{
   public static void main(String[] args)
   {
       int n=5;
       int r=2;
       int m=13;
       int fact1=facto(n);
       int fact2=facto(r);
       int fact3=facto(n-r);
       System.out.println(((int)fact1/(fact3*fact2))%m);
   }
   private static int facto(int n)
   {
       if(n==1)
	   {
	        return 1;
	   }
	   return n*facto(n-1);
   }
}

#include <stdio.h>

int main()
{
    int n,k=0;
    scanf("%d",&n);
    int arr[n],arr1[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++){
        int q=arr[i];
        int c=0;
        for(int j=1;j<=q;j++){
            if(q%j==0){
                c++;
            }
        }
        arr1[k++]=c;
    }
    for(int i=0;i<k;i++){
        printf("%d ",arr1[i]);
    }

    return 0;
}
